ading: false,\n    };\n  },\n  methods: {\n    async fetchData() {\n      this.isLoading = true;\n      const { data } = await fetchPosts();\n      this.isLoading = false;\n      this.postItems = data.posts;\n    },\n  },\n  created() {\n    this.fetchData();\n  },\n};\n</script>\n\n<style></style>\n"]}]}